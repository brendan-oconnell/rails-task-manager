<h1>Add A New Task</h1>

<%= form_for(@task) do |t| %>
  <%= t.label :title %>
  <%= t.text_field :title %>
  <%= t.label :details %>
  <%= t.text_field :details %>
  <%= t.submit %>
<% end %>

 <button type="button" class="btn btn-default">
 <%= link_to("Back to Task List", tasks_path) %>
 </button>
